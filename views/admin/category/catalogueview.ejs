<%- include('../../front/header.ejs') %>

<section class="dashboard-wrap mtb-40">
    <div class="container">
        <div class="body-content">
            <div class="row">
                <div class="col-md-3">
                    <div class="dash-left">
                        <ul>
                            <li class="active"><a href="/admin/dashboard" class="active">Dashboard</a></li>
                            <li><a href="/admin/categorydisplay">Catalogue Blogs</a></li>
                            <li><a href="/logout">Logout</a></li>
                        </ul>
                    </div>   
                </div>
                <div class="col-md-9">
                    <div class="dash-right">
                        <div class="dash-header">
                            <div class="dash-title">
                                <h1>View Catalogue</h1>
                            </div>
                            <div class="dash-nav">
                                <a href="/admin/createcategory" class="btn btn-success">Add Category</a>
                            </div>
                        </div>
                        <table class="table table-bordered table-striped">
                            <tr>
                                <th>#ID</th>
                                <th>Title</th>
                                <th>Description</th>
                                <th>Name</th>
                                <th>Email</th>
                                <th>Image</th>
                            </tr>
                           
                                <tr>
                                    <td><%= data._id %></td>
                                    <td><%= data.title %></td>
                                    <td><%= data.description %></td>
                                    <td><%= data.name %></td>
                                    <td><%= data.email %></td>
                                    <td><img src="<%= data.catalogueImage.url %>"></td>
                                    
                                </tr>
                           
                        </table>
                    </div>   
                </div>
            </div>
        </div>
    </div>
</section>

<%- include('../../front/footer.ejs') %>